  <INTERFACE __key="interface">
    <STATEDEF __key="states" category="Global" desc="The duration of each block of data. Each 'run' iteration will simulate this amount of time. Usually set to the value of (2^n)/BitRateDefault, where n is an integer. The ultimate spectral resolution will be equal to 1/TimeWindow." label="TimeWindow" objectId="TimeWindow" type="float">
      <SETTING __key="default" exprType="standard" state="TimeWindow" unit="s" value="64/BitRateDefault" />
      <RANGEDEF __key="range" action="abort" bounds="open" maximum="inf" minimum="0" />
      <UNITDEF __key="unit" type="float" unit="s" />
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="Allows simulations with the number of the signal samples whose Greatest Prime Factor (GPF) does not exceed the specified limit. For example, for 150 samples the GPF is equal to 5 (150 = 2x3x5x5). If the signal is resampled during simulation the new sample rate may be automatically adjusted (increased) to satisfy the above condition. Holding the GPF limit is important to ensure high speed of the Fast Fourier Transform that is widely used in simulations. The highest speed is achieved with the default setting 2, which is equivalent to the condition that the number of samples is an integer power of two. It is recommended to set the GPF limit not larger than 13. Setting it to -1 will deactivate the GPF limiting, which allows to simulate an arbitrary number of samples in the signal." label="GreatestPrimeFactorLimit" objectId="GreatestPrimeFactorLimit" type="int">
      <SETTING __key="default" exprType="standard" state="GreatestPrimeFactorLimit" value="2" />
      <RANGEDEF __key="range" action="abort" bounds="open" maximum="inf" minimum="-1" />
      <UNITDEF __key="unit" type="int" unit="" />
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="If OFF, noise generated by amplifiers will be added to the sampled bands, where they exist, as random fields (and will be added to Noise Bins elsewhere in the spectrum). If ON, this noise will be kept as the statistics in Noise Bins (that is, NoiseBins will cover the entire spectrum). Set to ON for deterministic BER estimation." label="InBandNoiseBins" objectId="InBandNoiseBins" type="enumeration">
      <SETTING __key="default" exprType="standard" state="InBandNoiseBins" value="OFF" />
      <ENUMDEF __key="enumerations" value="ON" />
      <ENUMDEF __key="enumerations" value="OFF" />
      <UNITDEF __key="unit" type="enumeration" unit="" />
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="Periodic Boundary Conditions should be used in most cases; each Block will represent a separate 'experiment' representing all time. Aperiodic Boundary Conditions are used for Photonic Circuits simulations, as well as for WDM simulations studying transients in EDFAs, semiconductor lasers and amplifiers and in some other applications, where subsequent blocks represent parts of a continuing waveform. Mixed Boundary Conditions should be used when it is necessary to combine different types of boundary conditions in one simulation." label="BoundaryConditions" objectId="BoundaryConditions" type="enumeration">
      <SETTING __key="default" exprType="standard" state="BoundaryConditions" value="Periodic" />
      <ENUMDEF __key="enumerations" value="Periodic" />
      <ENUMDEF __key="enumerations" value="Aperiodic" />
      <ENUMDEF __key="enumerations" value="Mixed" />
      <UNITDEF __key="unit" type="enumeration" unit="" />
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="Logical information is generated by transmitters and used to reconstruct the transmitted data for BER estimation and clock recovery. If set to ON, logical information is generated and kept in memory for each simulation run. If set to CurrentRun, only the information for the current run is kept (data from previous runs is removed to reduce memory consumption). If set to OFF, logical information is not generated. LogicalInformation = OFF is particularly useful for Photonic Circuits simulations." label="LogicalInformation" objectId="LogicalInformation" type="enumeration">
      <SETTING __key="default" exprType="standard" state="LogicalInformation" value="ON" />
      <ENUMDEF __key="enumerations" value="ON" />
      <ENUMDEF __key="enumerations" value="OFF" />
      <ENUMDEF __key="enumerations" value="CurrentRun" />
      <UNITDEF __key="unit" type="enumeration" unit="" />
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="This sets the simulated bandwidth (the sample rate) of transmitters and modules when producing 'samples' at their outputs (OutputDataType = samples). This parameter is typically only used in Photonic Circuits simulations ('Sample Mode'). If OutputDataType = samples, the SampleRate parameter of transmitter modules is overridden by SampleModeBandwidth." label="SampleModeBandwidth" objectId="SampleModeBandwidth" type="float">
      <SETTING __key="default" exprType="standard" state="SampleModeBandwidth" unit="Hz" value="SampleRateDefault" />
      <RANGEDEF __key="range" action="abort" bounds="open" maximum="inf" minimum="0" />
      <UNITDEF __key="unit" type="float" unit="Hz" />
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="This sets the center optical frequency of the simulated bandwidth of transmitters and modules when producing 'samples' at their outputs (OutputDataType = samples). This parameter is typically only used in Photonic Circuits simulations ('Sample Mode'). In other simulations, the simulated bandwidth is centered around the emission frequency." label="SampleModeCenterFrequency" objectId="SampleModeCenterFrequency" type="float">
      <SETTING __key="default" exprType="standard" state="SampleModeCenterFrequency" unit="Hz" value="193.1e12" />
      <RANGEDEF __key="range" action="abort" bounds="open" maximum="inf" minimum="0" />
      <UNITDEF __key="unit" type="float" unit="Hz" />
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="This parameter is passed by reference to many transmitter and other modules (their default SampleRate = SampleRateDefault), and so can be used to set the simulation bandwidth of each channel. Usually set to the value of (2^m)*BitRateDefault, where m is an integer (typically 4,5,6). If channel simulation bands overlap, they will combine." label="SampleRateDefault" objectId="SampleRateDefault" type="float">
      <SETTING __key="default" exprType="standard" state="SampleRateDefault" unit="Hz" value="512*BitRateDefault" />
      <RANGEDEF __key="range" action="abort" bounds="open" maximum="inf" minimum="0" />
      <UNITDEF __key="unit" type="float" unit="Hz" />
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="This parameter is passed by reference to many transmitter modules (their default BitRate = BitRateDefault), and so can be used to set the bit rate of each channel. Usually set to 2.5e9, 10e9 or 40e9." label="BitRateDefault" objectId="BitRateDefault" type="float">
      <SETTING __key="default" exprType="standard" state="BitRateDefault" unit="bit/s" value="10e9" />
      <RANGEDEF __key="range" action="abort" bounds="open" maximum="inf" minimum="0" />
      <UNITDEF __key="unit" type="float" unit="bit/s" />
    </STATEDEF>
    <STATEDEF __key="states" category="DesignRules" desc="This defines how the tracking of parameterized signals properties is performed. If set to None, no tracking is performed. If set to FinalValues, only final values are saved. If set to Detailed, the tracking information about the parameterized signal properties is saved along the propagation path." label="TrackingMode" objectId="TrackingMode" type="enumeration">
      <SETTING __key="default" exprType="standard" state="TrackingMode" value="None" />
      <ENUMDEF __key="enumerations" value="None" />
      <ENUMDEF __key="enumerations" value="FinalValues" />
      <ENUMDEF __key="enumerations" value="Detailed" />
      <UNITDEF __key="unit" type="enumeration" unit="" />
    </STATEDEF>
    <STATEDEF __key="states" category="DesignRules" desc="This defines the interval over z of saving the signal properties in distributed modules (fibers)" label="TrackingStepSize" objectId="TrackingStepSize" type="float">
      <CONTEXTDEF __key="context" enumeration="" expression="( $TrackingMode==&quot;Detailed&quot; )" value="" />
      <SETTING __key="default" exprType="standard" state="TrackingStepSize" unit="m" value="1e4" />
      <RANGEDEF __key="range" action="abort" bounds="open" maximum="inf" minimum="0" />
      <UNITDEF __key="unit" type="float" unit="m" />
    </STATEDEF>
    <STATEDEF __key="states" category="DesignRules" desc="This defines the initial value for effective optical bandwidth used for BER calculation" label="OpticalBandwidth" objectId="OpticalBandwidth" type="float">
      <CONTEXTDEF __key="context" enumeration="" expression="($TrackingMode != &quot;None&quot;)" value="" />
      <SETTING __key="default" exprType="standard" state="OpticalBandwidth" unit="Hz" value="4*BitRateDefault" />
      <RANGEDEF __key="range" action="abort" bounds="open" maximum="inf" minimum="0" />
      <UNITDEF __key="unit" type="float" unit="Hz" />
    </STATEDEF>
    <STATEDEF __key="states" category="DesignRules" desc="This defines the initial value for effective electrical bandwidth used for BER calculation" label="ElectricalBandwidth" objectId="ElectricalBandwidth" type="float">
      <CONTEXTDEF __key="context" enumeration="" expression="($TrackingMode != &quot;None&quot;)" value="" />
      <SETTING __key="default" exprType="standard" state="ElectricalBandwidth" unit="Hz" value="0.75*BitRateDefault" />
      <RANGEDEF __key="range" action="abort" bounds="open" maximum="inf" minimum="0" />
      <UNITDEF __key="unit" type="float" unit="Hz" />
    </STATEDEF>
  </INTERFACE>
